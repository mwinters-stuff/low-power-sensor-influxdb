{
  // See https://go.microsoft.com/fwlink/?LinkId=733558
  // for the documentation about the tasks.json format
  "version": "2.0.0",
  "tasks": [
    {
      "label": "make",
      "type": "process",
      "command": "make",
      "options": {
        "cwd": "${workspaceFolder}"
      },
      "args": [
        "-j",
        "10"
      ],
      "group": {
        "kind": "build",
        "isDefault": true
      },
      "problemMatcher": "$gcc"
    },
    {
      "label": "Decode Stack",
      "type": "shell",
      "command": "espdecodestack.sh",
      "args": [
        "build.nodemcuv2-2.5.2/low-power-sensor-influxdb.elf",
        "stack.txt",
        ">>",
        "stack.txt"
      ],
      "problemMatcher": []
    }
  ]
}